<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>employee_management.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>createEmployee</servlet-name>
  	<display-name>createEmployee</display-name>
  	<description></description>
  	<servlet-class>employee_management.createEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>list</servlet-name>
  	<display-name>list</display-name>
  	<description></description>
  	<servlet-class>employee_management.list</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updateEmployee</servlet-name>
  	<display-name>updateEmployee</display-name>
  	<description></description>
  	<servlet-class>employee_management.updateEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>searchdelete</servlet-name>
  	<display-name>searchdelete</display-name>
  	<description></description>
  	<servlet-class>employee_management.searchdelete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteEmployee</servlet-name>
  	<display-name>deleteEmployee</display-name>
  	<description></description>
  	<servlet-class>employee_management.deleteEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login_page</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>createEmployee</servlet-name>
  	<url-pattern>/createEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>list</servlet-name>
  	<url-pattern>/list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateEmployee</servlet-name>
  	<url-pattern>/updateEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>searchdelete</servlet-name>
  	<url-pattern>/searchdelete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteEmployee</servlet-name>
  	<url-pattern>/deleteEmployee</url-pattern>
  </servlet-mapping>
  </web-app>
